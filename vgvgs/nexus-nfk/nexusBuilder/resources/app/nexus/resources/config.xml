<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="/home/remizero/Proyectos/Desarrollo/Qt/qtrcp/com/ecosoftware/resources/xsd/config.xsd">
  <app>
    <applicationdisplayname>{{applicationdisplayname}}</applicationdisplayname>
    <applicationhashkey>{{applicationhashkey}}</applicationhashkey>
    <applicationname>{{applicationname}}</applicationname>
    <applicationversion>1.0</applicationversion>
    <applicationdescription>{{applicationdescription}}</applicationdescription>
    <organizationname>{{organizationname}}</organizationname>
    <organizationdomain>{{organizationdomain}}</organizationdomain>
    <showsplash>0</showsplash>
    <showtryicon>0</showtryicon>
    <standalone>0</standalone>
    <iconimage>{{iconimage}}</iconimage>
    <mayorversion>5</mayorversion>
    <minorversion>0</minorversion>
    <pathversion>0</pathversion>
    <typeApp>core</typeApp>
    <userConfigFormat>xml</userConfigFormat>
  </app>
  <commands>
    <command1>{{command1}}</command1>
  </commands>
  <logger>
    <sender>{{sender}}</sender>
    <sendername>{{sendername}}</sendername>
    <addressee>{{addressee}}</addressee>
    <addresseename>{{addresseename}}</addresseename>
    <subject>{{subject}}</subject>
    <smtpserver>{{smtpserver}}</smtpserver>
    <port>{{port}}</port>
    <user>{{user}}</user>
    <password>{{password}}</password>
  </logger>
  <systemtrayicon>
    <systrayicon>{{systrayicon}}</systrayicon>
    <systraytooltip>{{systraytooltip}}</systraytooltip>
    <systrayduration>10</systrayduration>
    <systraydefaultmessagetype>{{systraydefaultmessagetype}}</systraydefaultmessagetype>
    <systraymessagetitle>{{systraymessagetitle}}</systraymessagetitle>
    <systraymessagebody>{{systraymessagebody}}</systraymessagebody>
    <showsystemtrayicon>1</showsystemtrayicon>
    <minimizetosystemtrayicon>1</minimizetosystemtrayicon>
  </systemtrayicon>
  <splashscreen>
    <imagesplash>{{imagesplash}}</imagesplash><!--Aquí coloque el nombre de la imagen que usará para este lanzador de aplicaciones. Puede utilizar imagenes de tipo GIF, JPG, BMP, PNG.-->
    <messagebox>
      <x>10</x>
      <y>253</y>
      <w>800</w>
      <h>14</h>
      <alignment>1</alignment>
    </messagebox>
    <font>
      <family>monospace</family>
      <bold>1</bold>
      <size>9</size>
      <stretch>125</stretch>
      <color>2a5c7e</color>
    </font>
    <progressbar>
      <textvisible>0</textvisible><!-- Indica si el texto interno de la barra de progreso será visible -->
      <textalignment>center</textalignment><!-- Puede ser top | bottom | left | right | center -->
      <invertedappearance>0</invertedappearance><!-- This property holds whether or not a progress bar shows its progress inverted (e.g. from right to left). -->
      <x>0</x><!-- Pocisión en el eje X de la barra de progreso -->
      <y>270</y><!-- Pocisión en el eje Y de la barra de progreso -->
      <w>800</w><!-- Ancho de la barra de progreso -->
      <h>3</h><!-- Altura de la barra de progreso -->
      <orientation>H</orientation><!-- This property holds the orientation of the progress bar. The orientation must be Qt::Horizontal (the default) or Qt::Vertical. -->
      <textdirection>0</textdirection><!-- This property holds the reading direction of the text for vertical progress bars. -->
      <format>%p%</format><!-- %p% - is replaced by the percentage completed. %v% - is replaced by the current value. %m% - is replaced by the total number of steps. -->
      <max>100</max><!-- Valor máximo que puede aceptar la barra de progreso -->
      <min>0</min><!-- Valor mínimo que puede aceptar la barra de progreso -->
      <backgroundcolor>2a5c7e</backgroundcolor><!-- Color a ser mostrado en la barra de progreso -->
    </progressbar>
  </splashscreen>
</settings>
